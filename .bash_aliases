alias l="ls"
alias ls="ls -lGH"
alias e="exit"
alias c="cd"
alias o="xdg-open"
alias v="vim"
alias evim="vim ~/.config/neovim/init.vim"
alias g="git"
alias gcb="git checkout -b"
alias gaa="git add -A"
alias gs="git status"
alias gcl="git clean -fd"
alias gdab="git branch | grep -v "master" | xargs git branch -D"
alias gmm="git merge origin/master"
alias grb="git rebase origin/master"
alias uu="sudo apt update && sudo apt upgrade -y && sudo apt autoremove -y && sudo apt autoclean -y"
alias sll="while true; do sl -e || break; done"
alias fixaudio="pulseaudio -k && sudo alsa force-reload"
alias dock="docker run -it --rm ubuntu:18.04 /bin/bash"

